{
  "name": "JsteManager",
  "version": "0.1.0",
  "description": "The official Jste Manager.",
  "main": "src/index.js",
  "bin": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "build": "pkg ./src/index.js --out-path ./build -c ./package.json",
    "test": "pkg ./src/index.js --out-path ./build -c ./package.json"
  },
  "dependencies": {
    "express": "",
    "http-static-gzip-regexp": "",
    "cors": "",
    "appdirectory": "",
    "node-json-db": "",
    "remote-file-size": "",
    "xmlhttprequest": "",
    "body-parser": "",
    "express-device": "",
    "pkg": "",
    "figlet": "",
    "shelljs": "",
    "command-line-args": "",
    "cors-anywhere": "",
    "serve-favicon": ""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/project-jste/manager"
  },
  "pkg": {
    "assets": [
      "src/assets/*"
    ],
    "targets": [
      "node-latest-linux-x86",
      "node-latest-linux-x64",
      "node-latest-alpine-x64",
      "node-latest-win-x86",
      "node-latest-win-x64",
      "node-latest-macos-x86",
      "node-latest-macos-x64"
    ]
  },
  "keywords": [
    "node",
    "jste",
    "framework",
    "js",
    "html",
    "css"
  ],
  "author": "Jste Team",
  "homepage": "https://project-jste.github.io/",
  "license": "AGPL-3.0+"
}
